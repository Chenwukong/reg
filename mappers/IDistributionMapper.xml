<mapper namespace="com.demo.shop_demo.dao.IDistributionDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.demo.shop_demo.entity.Distribution">
        <id column="did" property="did" />
